@page "/"
@namespace BlazorCrudDemo.Web.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "_Layout";
}

<!-- Add Syncfusion Script Manager -->
<!-- <component type="typeof(Syncfusion.Blazor.ScriptManager)" render-mode="ServerPrerendered" /> -->

<component type="typeof(BlazorCrudDemo.Web.App)" render-mode="ServerPrerendered" />

<!-- Add Font Awesome -->
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" />

<!-- Add Chart.js for interactive charts -->
<script src="https://cdn.jsdelivr.net/npm/chart.js"></script>

<!-- Add custom dashboard charts JavaScript -->
<script src="~/js/dashboard-charts.js"></script>

<!-- Add Syncfusion Essential JS 2 styles -->
<link href="https://cdn.syncfusion.com/blazor/25.1.34/styles/bootstrap5.css" rel="stylesheet" />
